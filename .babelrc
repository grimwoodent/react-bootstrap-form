{
  "exclude": "node_modules/**",
  "presets": [
    ["@babel/preset-env", {
      "modules": 'commonjs',
      "targets": {
        "browsers": ["last 2 versions", "ie >= 9"]
      }
    }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-function-bind"
  ]
}
